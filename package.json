{
  "name": "teamenver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "generate": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=./.env.local",
    "gen": "pnpm generate",
    "commit": "cz",
    "check:lint": "next lint",
    "check:typecheck": "tsc --noEmit",
    "check:markuplint": "markuplint \"./src/**/*.{jsx,tsx}\"",
    "check:format": "prettier \"./src/**/*.ts\" \"./src/**/*.tsx\" --check",
    "format": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "test:storybook": "test-storybook --url http://localhost:6006",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "bunx http-server storybook-static -a localhost -p 6006 -s",
    "storybook:test": "bunx wait-on http://localhost:6006 && bun run test:storybook",
    "storybook:ci": "bunx concurrently -k -s first \"bun run storybook:serve\" \"bun run storybook:test\"",
    "chromatic": "chromatic --exit-zero-on-changes ---build-script-name \"storybook:build\"",
    "precommit": "lint-staged",
    "commitlint": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "check:lint --dir src --fix",
      "check:markuplint",
      "check:typecheck",
      "format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@apollo/experimental-nextjs-app-support": "^0.4.3",
